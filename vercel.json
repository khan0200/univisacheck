{
  "crons": [
    {
      "path": "/api/cron-auto-check",
      "schedule": "*/30 * * * *"
    }
  ],
  "rewrites": [
    {
      "source": "/api/check-status",
      "destination": "/api/index.js"
    },
    {
      "source": "/api/check-status/:path*",
      "destination": "/api/[...path].js"
    }
  ]
}
